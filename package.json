{
	"name": "open-coverage",
	"displayName": "Open Coverage",
	"description": "Open the index.html for simplecov(https://github.com/simplecov-ruby/simplecov) ruby gem",
  "version": "0.0.1",
  "publisher": "phuocph",
  "repository": {
    "type": "git",
    "url": "https://github.com/phuocph/open-coverage"
  },
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:open-coverage.open"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-coverage.open",
				"title": "Open Coverage: Open"
			}
    ],
    "keybindings": [
      {
        "command": "open-coverage.open",
        "key": "ctrl+a ctrl+s",
        "mac": "cmd+a cmd+s"
      }
    ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
